<!doctype html><html><head><meta charset="utf-8"><title>Case List</title><link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet"></head><body class="p-4"><div class="container"><h1>Case List</h1><p><a href="/cases/new" class="btn btn-primary">Create New Case</a></p><table class="table table-bordered"><thead><tr><th>Client</th><th>Invoice</th><th>Amount</th><th>Due Date</th><th>Status</th><th>Action</th></tr></thead><tbody><% cases.forEach(function(c){ %><tr><td><%= clientsMap[c.client_id] %></td><td><%= c.invoice_number %></td><td><%= c.invoice_amount %></td><td><%= c.due_date %></td><td><%= c.status %></td><td><a href="/cases/<%= c.id %>" class="btn btn-sm btn-secondary">View</a></td></tr><% }) %></tbody></table></div></body></html>